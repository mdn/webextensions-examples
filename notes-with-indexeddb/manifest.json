{
  "description": "Examplifies working with IndexedDB in a background page",
  "manifest_version": 2,
  "name": "Notes with IndexedDB",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/notes-with-indexeddb",
  "icons": {
    "48": "icons/notebook.svg"
  },

  "applications": {
    "gecko": {
      "id": "notes-with-indexeddb@mozilla.org",
      "strict_min_version": "45.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/notebook.svg"
  }
}