{
  "manifest_version": 2,
  "icons": {
    "16": "images/whimsycorn-18.png",
    "19": "images/whimsycorn-48.png",
    "32": "images/whimsycorn-32.png",
    "38": "images/whimsycorn-128.png",
    "48": "images/whimsycorn-48.png",
    "64": "images/whimsycorn-64.png",
    "128": "images/whimsycorn-128.png"
  },
  "applications": {
    "gecko": {
      "id": "ui-playground@mozilla",
      "strict_min_version": "40.0.0"
    }
  },
  "permissions": [
    "tabs",
    "alarms",
    "notifications",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*",
        "*://*.chilloutandwatchsomecatgifs.com/*"
      ],
      "js": [
        "lib/notify.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "lib/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/whimsycorn-18.png",
      "19": "images/whimsycorn-48.png",
      "32": "images/whimsycorn-32.png",
      "38": "images/whimsycorn-128.png",
      "48": "images/whimsycorn-48.png",
      "64": "images/whimsycorn-64.png",
      "128": "images/whimsycorn-128.png"
    },
    "default_title": "Whimsy!  ‚òïÔ∏è ‚ú® üö≤",
    "default_popup": "html/catgifs.html"
  },
  "page_action": {
    "default_icon": {
      "16": "images/whimsycorn-18.png",
      "19": "images/whimsycorn-48.png",
      "32": "images/whimsycorn-32.png",
      "38": "images/whimsycorn-128.png",
      "48": "images/whimsycorn-48.png",
      "64": "images/whimsycorn-64.png",
      "128": "images/whimsycorn-128.png"
    },
    "default_title": "Whimsy!  ‚òïÔ∏è ‚ú® üö≤",
    "default_popup": "html/catgifs.html"
  },
  "devtools_page": "html/devtools.html",
  "name": "UI Playground ‚òïÔ∏è ‚ú® üö≤",
  "version": "0.0.1",
  "description": "Test various WebExtension features. ‚òïÔ∏è ‚ú® üö≤",
  "author": "Blake Winton <bwinton@latte.ca>",
  "homepage_url": "https://github.com/bwinton/whimsy"
}
