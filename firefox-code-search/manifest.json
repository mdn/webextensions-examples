{
  "name": "Firefox Code Search",
  "description" : "To use, type 'cs' plus a search term into the url bar.",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "52.0a1"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "omnibox": { "keyword" : "cs" },
  "manifest_version": 2,
  "permissions": [
    "https://searchfox.org/"
  ]
}
